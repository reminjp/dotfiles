[alias]
	current-branch-name = !git branch | grep -e '^\\* ' | sed -e 's/^\\* //g'
	force-pull = !git fetch && git reset --hard origin/$(git current-branch-name)
	logg = log --graph --pretty=oneline
[color]
	ui = auto
[core]
	autocrlf = input
	editor = vim
	symlinks = true
[merge]
	ff = false
[pull]
	ff = only
[push]
	default = simple
[user]
	email = rdrgn@users.noreply.github.com
	name = remin
	username = rdrgn
